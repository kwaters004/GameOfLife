        <Button x:Name="button24" Grid.Column="4" Grid.Row="5" Width="150" Height="150" Margin="1" Background="White" Click="button_Click" />
        <Button x:Name="button25" Grid.Column="5" Grid.Row="5" Width="150" Height="150" Margin="1" Background="White" Click="button_Click" />
        <Button x:Name="NextButton" Grid.Column="3" Grid.Row="6" Height="50" Margin="10" Background="#FFA2A2A2" Content ="Next" Click="NextButton_Click"/>


    </Grid>
</Window>
