        <Button x:Name="button6" Grid.Column="1" Grid.Row="2" Width="150" Height="150" Margin="1" Background="White" Click="button_Click" />
        <Button x:Name="button7" Grid.Column="2" Grid.Row="2" Width="150" Height="150" Margin="1" Background="White" Click="button_Click" />
        <Button x:Name="button8" Grid.Column="3" Grid.Row="2" Width="150" Height="150" Margin="1" Background="White" Click="button_Click" />
        <Button x:Name="button9" Grid.Column="4" Grid.Row="2" Width="150" Height="150" Margin="1" Background="White" Click="button_Click" />
        <Button x:Name="button10" Grid.Column="5" Grid.Row="2" Width="150" Height="150" Margin="1" Background="White" Click="button_Click" />
        <Button x:Name="button11" Grid.Column="1" Grid.Row="3" Width="150" Height="150" Margin="1" Background="White" Click="button_Click" />
        <Button x:Name="button12" Grid.Column="2" Grid.Row="3" Width="150" Height="150" Margin="1" Background="White" Click="button_Click" />
        <Button x:Name="button13" Grid.Column="3" Grid.Row="3" Width="150" Height="150" Margin="1" Background="White" Click="button_Click" />
        <Button x:Name="button14" Grid.Column="4" Grid.Row="3" Width="150" Height="150" Margin="1" Background="White" Click="button_Click" />
        <Button x:Name="button15" Grid.Column="5" Grid.Row="3" Width="150" Height="150" Margin="1" Background="White" Click="button_Click" />
        <Button x:Name="button16" Grid.Column="1" Grid.Row="4" Width="150" Height="150" Margin="1" Background="White" Click="button_Click" />
        <Button x:Name="button17" Grid.Column="2" Grid.Row="4" Width="150" Height="150" Margin="1" Background="White" Click="button_Click" />
        <Button x:Name="button18" Grid.Column="3" Grid.Row="4" Width="150" Height="150" Margin="1" Background="White" Click="button_Click" />
        <Button x:Name="button19" Grid.Column="4" Grid.Row="4" Width="150" Height="150" Margin="1" Background="White" Click="button_Click" />
        <Button x:Name="button20" Grid.Column="5" Grid.Row="4" Width="150" Height="150" Margin="1" Background="White" Click="button_Click" />
        <Button x:Name="button21" Grid.Column="1" Grid.Row="5" Width="150" Height="150" Margin="1" Background="White" Click="button_Click" />
